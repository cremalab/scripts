"import { useState } from \\"react\\"

interface Props {
  name: string
}

export function C({ name }: Props) {
  const [count, setCount] = useState(0)

  return (
    <div>
      <p>Hi, my name is {name}!</p>
      <p>You clicked {count} times</p>
      <button onClick={() => setCount(count + 1)}>Click Me</button>
    </div>
  )
}
"
